FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.bubble 0
);

regions
(
    cylinderToCell
    {
        p1 (0 0 0);
        p2 (0 0 1);
        radius 0.1;

        fieldValues
        (
            volScalarFieldValue alpha.bubble 1
        );
    }
);


