FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    topWall
    {
        type            zeroGradient;
    }

    bottomWall
    {
        type            zeroGradient;
    }

//     roundWall
//     {
//         type            fixedValue;
//         value           uniform 0;
//     }
    roundWall
    {
        type            zeroGradient;
    }
    
    ypWall
    {
        type            wedge;
    }

    ymWall
    {
        type            wedge;
    }
    
    axisWall
    {
        type            empty;
    }
}


