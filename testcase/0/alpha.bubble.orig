FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.bubble;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    topWall
    {
        type            zeroGradient;
    }

    bottomWall
    {
        type            zeroGradient;
    }

    roundWall
    {
        type            zeroGradient;
    }
    
    ypWall
    {
        type            wedge;
    }
    
    ymWall
    {
        type            wedge;
    }

    axisWall
    {
        type            empty;
    }
}


