    topWall
    {
        type            zeroGradient;
    }

    bottomWall
    {
        type            zeroGradient;
    }

    roundWall
    {
        type            zeroGradient;
    }
    
    ymWall
    {
        type            wedge;
    }
    
    ypWall
    {
        type            wedge;
    }

    axisWall
    {
        type            empty;
    }
}


